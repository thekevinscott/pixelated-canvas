{
  "name": "pixelated-canvas",
  "version": "0.5.0",
  "description": "",
  "repository": "https://github.com/thekevinscott/pixelated-canvas",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "example": "parcel example/index.html -d docs --public-url /pixelated-canvas/",
    "develop": "concurrently \"npm:build:watch\" \"npm:example\"",
    "deploy": "gh-pages -d docs",
    "test": "jest --watchAll"
  },
  "author": "Kevin Scott <kevin@hitheory.com>",
  "license": "MIT",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@types/color": "^3.0.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.10",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-runtime": "^6.26.0",
    "concurrently": "^4.1.0",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "parcel": "^1.10.3",
    "rollup": "^0.67.4",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "color": "^3.1.0"
  }
}
